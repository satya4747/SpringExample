<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans  
               http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

	<bean id="empBean" class="com.training.springcore.model.Employee">
		<property name="name" value="Guest1"></property>
	</bean>
	
	<bean id="empBean1" class="com.training.springcore.model.Employee">
		<constructor-arg name="name" value="NIIT"></constructor-arg>
	</bean>

</beans>